{
  "name": "porn-manager",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "bin": "build/index.js",
  "scripts": {
    "install:app": "cd app && npm ci && cd ..",
    "transpile": "tsc",
    "watch": "tsc -w",
    "start": "node build",
    "mon": "NODE_ENV=development nodemon build/",
    "dev": "NODE_ENV=development node --max_old_space_size=8192 -r ts-node/register ./src/index.ts",
    "test": "exit 1",
    "build": "mkdir -p release/app/dist && npm run build:server && npm run build:app && cp -r ./views ./release && cp -r ./app/dist ./release/app",
    "build:app": "cd app && npm run build && cd ..",
    "build:server": "tsc && pkg . --targets node8 --options max_old_space_size=8192 --out-path ./release",
    "build:all": "npm run build:app && tsc && npm run build:linux && npm run build:win && npm run build:mac",
    "build:linux": "mkdir -p releases/linux/app/dist && pkg . --out-path ./releases/linux --targets node8-linux-x64 --options max_old_space_size=8192 && cp -r ./views ./releases/linux && cp -r ./app/dist ./releases/linux/app && npm run zip:linux",
    "build:win": "mkdir -p releases/win/app/dist && pkg . --out-path ./releases/win --targets node8-win-x64 --options max_old_space_size=8192 && cp -r ./views ./releases/win && cp -r ./app/dist ./releases/win/app && npm run zip:win",
    "build:mac": "mkdir -p releases/mac/app/dist && pkg . --out-path ./releases/mac --targets node8-macos-x64 --options max_old_space_size=8192 && cp -r ./views ./releases/mac && cp -r ./app/dist ./releases/mac/app && npm run zip:mac",
    "zip:linux": "zip -r ./releases/linux.zip ./releases/linux",
    "zip:win": "zip -r ./releases/win.zip ./releases/win",
    "zip:mac": "zip -r ./releases/mac.zip ./releases/mac"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boi123212321/porn-manager.git"
  },
  "author": "boi123212321",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boi123212321/porn-manager/issues"
  },
  "homepage": "https://github.com/boi123212321/porn-manager#readme",
  "dependencies": {
    "apollo-server-express": "^2.9.8",
    "axios": "^0.19.1",
    "checksum": "^0.1.1",
    "cheerio": "^1.0.0-rc.3",
    "cli-progress": "^3.6.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "graphql-tools": "^4.0.6",
    "graphql-type-json": "^0.3.1",
    "graphql-type-long": "^0.1.1",
    "inquirer": "7.0.0",
    "jimp": "^0.9.3",
    "js-sha512": "^0.8.0",
    "merge-img": "^2.1.3",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "nedb": "^1.8.0",
    "node-vibrant": "^3.1.5",
    "ora": "^4.0.3",
    "pug": "^2.0.4",
    "rimraf": "^3.0.0",
    "tiny-async-pool": "^1.1.0",
    "validate": "^5.1.0",
    "yaml": "^1.7.2",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/checksum": "^0.1.30",
    "@types/cli-progress": "^3.4.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/fluent-ffmpeg": "^2.1.13",
    "@types/graphql": "^14.5.0",
    "@types/inquirer": "^6.5.0",
    "@types/mkdirp": "^0.5.2",
    "@types/nedb": "^1.8.9",
    "@types/node": "^13.1.8",
    "@types/pug": "^2.0.4",
    "@types/rimraf": "^2.0.3",
    "@types/tiny-async-pool": "^1.0.0",
    "@types/yaml": "^1.2.0",
    "@types/yargs": "^15.0.1",
    "nodemon": "^2.0.2",
    "pkg": "^4.4.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "nodemonConfig": {
    "ignore": [
      "/**/*.db",
      "/**/config.json"
    ]
  }
}
