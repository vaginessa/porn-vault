    "graphql-tools": "^7.0.2",
    "graphql-type-json": "^0.3.2",
    "graphql-type-long": "^0.1.1",
    "graphql-upload": "^15.0.2",
    "handlebars": "^4.7.6",
    "inquirer": "9.0.1",
    "js-sha512": "^0.8.0",
    "lru-cache": "^6.0.0",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "node-vibrant": "^3.1.6",
    "ora": "^5.3.0",
    "semver": "^7.3.7",
    "tiny-async-pool": "^1.2.0",
    "ts-node": "^10.2.0",
    "typescript": "^4.7.4",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.5.0",
    "yaml": "^1.10.0",
